	<environments default="dev-pool">
		<environment id="dev-unpool">
			<transactionManager type="JDBC">
				<property name="" value=""/>
			</transactionManager>
			<dataSource type="UNPOOLED">
				<property name="driver" value="${driver}"/>
				<property name="url" value="${url}"/>
				<property name="username" value="${username}"/>
				<property name="password" value="${password}"/>
			</dataSource>
		</environment>

		<environment id="dev-pool">
			<transactionManager type="JDBC">
				<property name="" value=""/>
			</transactionManager>
			<dataSource type="POOLED">
				<property name="driver" value="${driver}"/>
				<property name="url" value="${url}"/>
				<property name="username" value="${username}"/>
				<property name="password" value="${password}"/>
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<!--resource 指定mapper xml 地址-->
<!--		<mapper resource="freedom/san/mybatis/usage/basic/xml/ClassesMapper.xml"/>-->
		<!--引用的xml文件必须对应接口名称路径名一致-->
		<!--		<mapper class=""/>-->
		<!--引用网络资源或者本地资源的全路径-->
		<!--		<mapper url=""/>-->
		<!--		引入一个包下面的全部xml文件,且XML文件名和接口名称必须相同,路径也需要相同-->
		<package name="freedom.san.mybatis.usage.basic.mapper"/>
	</mappers>
</configuration>
